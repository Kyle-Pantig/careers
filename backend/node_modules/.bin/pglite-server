#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*|*MINGW*|*MSYS*)
        if command -v cygpath > /dev/null 2>&1; then
            basedir=`cygpath -w "$basedir"`
        fi
    ;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/proc/cygdrive/c/Users/Lenovo/Desktop/2026/careers/backend/node_modules/@electric-sql/pglite-socket/dist/scripts/node_modules:/proc/cygdrive/c/Users/Lenovo/Desktop/2026/careers/backend/node_modules/@electric-sql/pglite-socket/dist/node_modules:/proc/cygdrive/c/Users/Lenovo/Desktop/2026/careers/backend/node_modules/@electric-sql/pglite-socket/node_modules:/proc/cygdrive/c/Users/Lenovo/Desktop/2026/careers/backend/node_modules/@electric-sql/node_modules:/proc/cygdrive/c/Users/Lenovo/Desktop/2026/careers/backend/node_modules:/proc/cygdrive/c/Users/Lenovo/Desktop/2026/careers/node_modules:/proc/cygdrive/c/Users/Lenovo/Desktop/2026/node_modules:/proc/cygdrive/c/Users/Lenovo/Desktop/node_modules:/proc/cygdrive/c/Users/Lenovo/node_modules:/proc/cygdrive/c/Users/node_modules:/proc/cygdrive/c/node_modules:/proc/cygdrive/c/Users/Lenovo/Desktop/2026/careers/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/proc/cygdrive/c/Users/Lenovo/Desktop/2026/careers/backend/node_modules/@electric-sql/pglite-socket/dist/scripts/node_modules:/proc/cygdrive/c/Users/Lenovo/Desktop/2026/careers/backend/node_modules/@electric-sql/pglite-socket/dist/node_modules:/proc/cygdrive/c/Users/Lenovo/Desktop/2026/careers/backend/node_modules/@electric-sql/pglite-socket/node_modules:/proc/cygdrive/c/Users/Lenovo/Desktop/2026/careers/backend/node_modules/@electric-sql/node_modules:/proc/cygdrive/c/Users/Lenovo/Desktop/2026/careers/backend/node_modules:/proc/cygdrive/c/Users/Lenovo/Desktop/2026/careers/node_modules:/proc/cygdrive/c/Users/Lenovo/Desktop/2026/node_modules:/proc/cygdrive/c/Users/Lenovo/Desktop/node_modules:/proc/cygdrive/c/Users/Lenovo/node_modules:/proc/cygdrive/c/Users/node_modules:/proc/cygdrive/c/node_modules:/proc/cygdrive/c/Users/Lenovo/Desktop/2026/careers/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../@electric-sql/pglite-socket/dist/scripts/server.js" "$@"
else
  exec node  "$basedir/../@electric-sql/pglite-socket/dist/scripts/server.js" "$@"
fi
